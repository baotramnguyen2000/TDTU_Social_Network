<div class="container-fluid">

<div class="row">
  <div class="col-sm-9 col-md-6 col-lg-8 p-3" id="tb">
      
  </div>
</div>

<div>
  <h5 class="card-title welcome">Chào mừng <b class="text-primary"><%= user.name %></b> đến với hệ thống mạng xã hội sinh viên do khoa CNTT quản lý!</h5>
</div>


<div class="row">
    <div class="col-sm-7 col-md-6 col-lg-8 p-3" id="postList">

        <!-- <div class="card">
            <div class="card-header text-center whitetext bg pt-4">
                <h5>BÀI ĐĂNG MỚI</h5>
            </div>
        </div> -->
        <% if (posts) { posts = posts.reverse(); posts.forEach((post, index) => { %>
        <div class="card mt-3 shadow p-3 mb-5 bg-white rounded" id="<%= post._id %>">
          <% if(user.name == post.Owner || user.role == 'admin') { %>
            <!--Dropdown edit, delete-->
            <div class="card-header bg-white">
              <div class="dropdown float-right" data-display="static" >
                <i class="fas fa-ellipsis-h" id="dropdownMenuLink" style="cursor: pointer;" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></i>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuLink">
                  <a class="dropdown-item post-update" id="post-update" data-toggle="modal" data-target="#modalUpdate" data-id="<%= post._id %>" style="cursor: pointer;">Chỉnh sửa bài viết</a>
                  <a class="dropdown-item post-delete" id="post-delete" data-toggle="modal" data-target="#confirm-delete-dialog" data-id="<%= post._id %>" style="cursor: pointer;">Xóa bài viết</a>
                </div>
              </div>
            </div>
          <% } %>

        
          <div class="card-body bg-white">
            <p class="card-title">
              <a href="/dashboard/timeline/<%- post.Owner %>">
                <img class="rounded" id="userImg" src="<%= user.avatar %> " width="40">
                <%- post.Owner %> 
              </a>              
            </p>

            <input type="hidden" id="postId" value="<%= post._id %>">
            
            <span class="card-text" id="<%= post._id %>1311" style="overflow-y: auto;"><%- post.content %></p>
            <span class="card-text"><small class="text-muted">đăng vào <%= post.createAt %> </small></span>

            <div class="bg-white p-2">
                  <div class="d-flex flex-row fs-12">
                      <div class="like p-2 cursor action-collapse" data-toggle="collapse" aria-expanded="true" aria-controls="collapse-1<%= post._id %>" href="#collapse-1<%= post._id %>">
                        <i class="far fa-comment-alt"></i>
                        <span class="ml-2" style="cursor: pointer;">Bình luận</span>
                      </div>
                  </div>
              </div>	
              <div id="collapse-1<%= post._id %>" class="bg-white p-2 collapse" data-parent="#myGroup">
                  <div class="input-group mb-3 ">                    
                    <input id="Comment" type="text" class="form-control"  placeholder="Nhập bình luận...">
                    <input type="hidden" id="input-comment" value="<%= post._id %>">
                    <div class="input-group-append">
                      <button class="btn btn-primary commentSubmit" id="commentsub">Gửi</button>
                    </div>
                  </div>

                  <br>
                  <ul id="<%= post._id %>ulul" class="fb-comments" style="width:100%;">

                      <% if (comments) {comments.reverse(), comments.forEach((comment) => { %>
                          <% if(comment.PostId == post._id || user.role == 'admin') { %> 
                              <li class="list-group-item mt-3 shadow p-3 mb-2 bg-white rounded" id="<%= comment._id %>">
                                  <h6 class="card-title" >
                                    <img class="rounded" src="<%= user.avatar %>" width="40" >
                                      <%= comment.Owner %> 
                                      <p class="card-text"><small class="text-muted"><%= comment.createAt %> </small></p>
                                  </h6>
                                  <h6 class="card-subtitle mb-2 text-muted"></h6>
                                  <p class="card-text" ><%= comment.content %> </p>
                                  <% if(user.name == comment.Owner) { %>
                                  <div class="card-footer text-muted bg-white" style="text-align: right;">
                                      <button type="button" id="commentdelete" data-id="<%= comment._id %>" class="btn btn-danger btn-sm commentdelete">Delete</button>
                                  </div>
                                  <% } %>
                              </li>
                          <% } %>
                      <% }) } %>
                  </ul>
              </div>
          </div>
        </div>
        <% }) } %>
    </div>

    <div class="col-sm-5 col-md-6 col-lg-4 p-3">
      <div class="card">
        <div class="card-header text-center whitetext bg pt-4">
          <h5>THÔNG BÁO NỔI BẬT</h5>
        </div>

